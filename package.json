{
  "name": "divaservices",
  "version": "0.1.0",
  "description": "DivaServices RESTFul Web Services for Document Image Analysis",
  "main": "server.js",
  "scripts": {
    "postinstall": "tsc -p .",
    "watch": "tsc -w -p .",
    "build": "tsc",
    "grunt": "grunt",
    "start": "NODE_ENV=dev && /etc/init.d/mongod start && node --max-old-space-size=4096 bin/www --dockerIp=134.21.72.225 --dockerPort=2375 --rootIp=134.21.72.48:8080 --sshUser=docker --sshPass=docker",
    "debug": "NODE_ENV=dev nodemon --max-old-space-size=4096 --inspect=0.0.0.0:9929 bin/www",
    "test": "NODE_ENV=dev mocha tests/*Tests.js",
    "doc": "jsdoc src/ -r -d doc/",
    "docker-debug": "/etc/init.d/mongod start && npm run debug -- --dockerIp=134.21.72.225 --dockerPort=2375 --rootIp=134.21.72.48:8080 --sshUser=docker --sshPass=docker",
    "docker-cleanup": "docker-compose run --rm web /code/scripts/cleanUp.sh",
    "docker-cleandata": "docker-compose run --rm web /code/scripts/cleanData.sh",
    "docker-install": "docker-compose run --rm web /code/scripts/setup.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunactic/DIVAServices.git"
  },
  "keywords": [
    "REST",
    "DIA"
  ],
  "author": "Marcel Wuersch",
  "license": "LGPL-2.1",
  "bugs": {
    "url": "https://github.com/lunactic/DIVAServices/issues"
  },
  "homepage": "https://github.com/lunactic/DIVAServices#readme",
  "engines": {
    "node": "~8.2"
  },
