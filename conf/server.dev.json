{
  "logger": {
    "appenders": [
      {
        "type": "file",
        "filename": "logs/server.log",
        "maxLogSize": 2048000,
        "backups": 5
      },
      {
        "type": "console"
      }
    ],
    "level": "DEBUG"
  },
  "server": {
    "timeout": 600000,
    "httpPort": 8080,
    "httpsPort": 8443,
    "maxFileSize": 20971520,
    "rootUrl":"divaservices.unifr.ch"
  },
  "images": {
    "storeResponse": false
  },
  "paths": {
    "imageRootPath": "/data/images",
    "matlabPath": "/usr/local/bin/matlab",
    "matlabScriptsPath": "/data/matlab",
    "jsonPath":"/data/json",
    "ocropyLanguageModelsPath": "/data/ocropyLanguageModels",
    "statisticsFile":"/data/statistics.json",
    "servicesInfoFile":"/data/json/services.json",
    "noisingXmlPath": "/data/noise/scripts/realistic.xml"
  },
  "reservedWords" :[
    "inputImage",
    "inputImageUrl",
    "outputImage",
    "resultFile",
    "matlabPath",
    "imageRootPath",
    "matlabScriptsPath",
    "host",
    "inputFileExtension",
    "outputFolder",
    "highlighter",
    "ocropyLanguageModelsPath",
    "startUp",
    "noisingXmlFile"
  ]
}
