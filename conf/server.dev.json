{
  "logger": {
    "appenders": [
      {
        "type": "file",
        "filename": "logs/server.log",
        "maxLogSize": 2048000,
        "backups": 5
      },
      {
        "type": "console"
      }
    ],
    "level": "DEBUG"
  },
  "server": {
    "timeout": 600000,
    "port": 8080,
    "maxFileSize": 20971520
  },
  "images": {
    "storeResponse": false
  },
  "paths": {
    "imageRootPath": "/data/images",
    "matlabPath": "/usr/local/bin/matlab",
    "matlabScriptsPath": "/data/matlab"
  },
  "reservedWords" :[
    "image",
    "matlabPath",
    "matlabScriptsPath",
    "inputFileExtension"
  ]
}
