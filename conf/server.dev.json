{
  "logger": {
    "appenders": {
      "file": {
        "type": "file",
        "filename": "logs/server.log",
        "maxLogSize": 2048000,
        "backups": 5
      },
      "console": {
        "type": "console"
      }
    },
    "categories": {
      "default": {
        "appenders": [
          "file",
          "console"
        ],
        "level": "debug"
      }
    }
  },
  "docker": {
    "host": "DOCKER_HOST_IP",
    "port": "DOCKER_HOST_PORT",
    "reportHost": "DOCKER_REPORT_HOST",
    "user":"docker",
    "group":"docker",
    "uid":"1000",
    "gid":"1000",
    "sshUser": "DOCKER_SSH_USER",
    "sshPass": "DOCKER_SSH_PASS",
    "maxContainers": 8,
    "maxMemory": 4096,
    "replacePaths": [
      "inputFolder",
      "outputFolder",
      "resultFile",
      "outputImage"
    ],
    "paths": {
      "inputFolder": "/data",
      "outputFolder": "/data/output",
      "resultFile": "/data/result.json",
      "outputImage": "/data/outputImage.png"
    }
  },
  "server": {
    "cwlSupport": true,
    "timeout": 600000,
    "httpPort": 8080,
    "httpsPort": 8443,
    "maxFileSize": 20971520,
    "rootUrl": "SERVER_ROOT_URL",
    "managementWhitelistIp": [
      "127.0.0.1",
      "::1",
      "192.168.1.1/24",
      "172.18.0.1/16"
    ]
  },
  "images": {
    "storeResponse": false
  },
  "remoteServer": {
    "user": "wuerschm",
    "ip": "diuf-cluster"
  },
  "paths": {
    "filesPath": "/divadata/files",
    "resultsPath": "/divadata/results",
    "testPath": "/divadata/testData",
    "jsonPath": "/divadata/json",
    "logPath": "/divadata/logs",
    "imageInfoFile": "/divadata/files/filesInfo.json",
    "dataInfoFile": "/divadata/data/dataInfo.json",
    "rootInfoFile": "/divadata/json/info.json",
    "servicesInfoFile": "/divadata/json/services.json",
    "swaggerFile": "/divadata/json/swagger.json",
    "executablePath": "/divadata/executables",
    "mcr2014b": "$LD_LIBRARY_PATH"
  },
  "remotePaths": {
    "jsonPath": "/data/json",
    "noisingXmlPath": "/data/noise/scripts/realistic.xml",
    "mcr2014b": "$LD_LIBRARY_PATH"
  },
  "reservedWords": [
    "file",
    "folder",
    "inputFolder",
    "inputImageUrl",
    "outputImage",
    "resultFile",
    "imageRootPath",
    "host",
    "inputFileExtension",
    "outputFolder",
    "highlighter",
    "startUp",
    "mcr2014b",
    "inputFile"
  ]
}