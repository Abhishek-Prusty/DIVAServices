{
  "logger": {
    "appenders": [
      {
        "type": "file",
        "filename": "logs/server.log",
        "maxLogSize": 2048000,
        "backups": 5
      },
      {
        "type": "console"
      }
    ],
    "level": "DEBUG"
  },
  "server": {
    "timeout": 600000,
    "port": 8080,
    "maxFileSize": 20971520,
    "rootUrl":"http://192.168.56.101:8080"
  },
  "images": {
    "storeResponse": false
  },
  "paths": {
    "imageRootPath": "/data/images",
    "matlabPath": "/usr/local/bin/matlab",
    "matlabScriptsPath": "/data/matlab",
    "jsonPath":"/data/json",
    "ocropyLanguageModelsPath": "/data/ocropyLanguageModels",
    "statisticsFile":"/data/statistics.json",
    "servicesInfoFile":"/data/json/services.json"
  },
  "reservedWords" :[
    "image",
    "matlabPath",
    "imageRootPath",
    "matlabScriptsPath",
    "host",
    "inputFileExtension",
    "outputFolder",
    "highlighter",
    "ocropyLanguageModelsPath",
    "startUp"
  ]
}
